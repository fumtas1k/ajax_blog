<h1>Sign Up</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |form| %>
  <div class="form-group">
    <%= form.label :email, class: "sr-only" %>
    <%= form.email_field :email, class: "form-control", required: true, autofocus: true %>
    <%= form.label :password, class: "sr-only" %>
    <%= form.password_field :password, class:"form-control", required: true %>
  </div>
  <%= form.submit nil, class: "btn btn-primary" %>
  <%= link_to "アカウント登録", new_user_path, class: "btn btn-success" %>
<% end %>